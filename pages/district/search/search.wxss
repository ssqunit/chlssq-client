/* pages/district/search/search.wxss */
.container{width: 100%;height: auto;}
.area_ctn{width: 96%;height: auto;margin-left: 2%;margin-top: 10rpx;}
.space_line{width: 100%;height: 2rpx;background-color:gainsboro;margin:3rpx 0rpx;}
.fail_text{width: 100%;height: 130rpx;display: flex;flex-direction: column;font-size: 24rpx;padding-top: 30rpx;}
.list_ctn{width: 100%;height: auto;}
.item_ctn{width: 100%;height: 120rpx;display: flex;flex-direction: row;align-items: center;
border: 1rpx solid gainsboro;border-radius: 10rpx;margin: 10rpx 0rpx;}
.item_pos{width: 20%;height: 100%;display: flex;flex-direction: column;align-items: center;}
.pos_img{height: 70%;scale: 0.6;}
.pos_km{font-size: 22rpx;}
.item_img{width: 20%;height: 100%;background-color: rgb(241, 241, 241);}
.img_img{width: 100%;height: 100%;}
.item_name{width: 40%;height: 100%;display: flex;flex-direction: column;padding-left: 10rpx;}
.name_name{font-size: 32rpx;margin-top: 20rpx;}
.name_area{font-size: 24rpx;}
.item_btn{width: 20%;height: 100%;display: flex;flex-direction: row;align-items: center;}
.btn_btn{width: 96%;border: 0rpx;}
.bigspace{width: 100%;height: 70rpx;}

