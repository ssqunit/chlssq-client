<!--pages/message.wxml-->
<view class="container">
  <view hidden="{{msgList == null ? false : true}}" style="margin-top:20rpx;">
    <text>还没有收到任何消息！</text>
  </view>
  <van-dialog id="van-dialog" />
  <view class="list_ctn">
    <scroll-view>
      <block wx:for="{{msgList}}" wx:for-index="index">
      <ssq-msg-item cid="{{item.id}}" ctitle="{{item.title}}" cdate="{{item.date}}" cstatus="{{item.status}}" ccontent="{{item.content}}"/>
      </block>
    </scroll-view>
  </view>

</view>